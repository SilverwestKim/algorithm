   "source": [
    "def solution(A,K):\n",
    "    if len(A)> K:\n",
    "        return(A[len(A)-K:]+A[:len(A)-K])\n",
    "    elif len(A)==0 or len(A) == K :\n",
    "        return(A)\n",
    "    else :\n",
    "        K=K%len(A); \n",
    "        x=solution(A,K)\n",
    "        return(x)"
   ]
